<Window x:Class="GeoLib.WindowsHost.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <StackPanel Orientation="Vertical" RenderTransformOrigin="0.58,0.503">
        <Button x:Name="btnStart" Content="Start Services" Margin="50,20,20,0" FontSize="18" Click="btnStart_Click"  ></Button>
        <Button x:Name="btnStop" Content="Stop Services" Margin="50,20,20,0" FontSize="18" Click="btnStop_Click"  ></Button>
        <Label x:Name="lblMessage"   Margin="50,20,50,0" FontSize="18" HorizontalAlignment="Left"  ></Label>
    </StackPanel>
</Window>
